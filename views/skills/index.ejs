<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dev Skills</title>
    <link rel="public/stylesheet" type="text/css" href="style.css">
</head>
<body>
    <h1>Welcome to Dev Skills</h1>
    <a href ="/skills">Go Back Home</a>
    <ul>
        <% skills.forEach(function(skill, idx) { %>
            <li>
                <a href="/skills/<%=idx%>"> <%= skill.title%></a>
            </li>
            <% }); %>
        </ul>
    <a href="/skills/new">Add Skill</a>
</body>
</html>